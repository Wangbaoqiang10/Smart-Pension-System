    456              SampleApp_TxBuf[4+len]='@';
    457          
    458              //发送长度
    459              SampleApp_TxLen=5+len;
    460          
    461              //接着发数据包
    462              HalUARTWrite(0,SampleApp_TxBuf, SampleApp_TxLen);
    463          }
